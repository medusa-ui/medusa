<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss} [%thread] %highlight([%level]) %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="reactor.netty.http.client.HttpClientConnect" level="ERROR" />
    <root level="INFO">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>